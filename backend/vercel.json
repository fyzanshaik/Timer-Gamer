{
	"version": 2,
	"builds": [
		{
			"src": "src/app.ts",
			"use": "@vercel/node",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/dist/app.js"
		}
	],
	"env": {
		"DATABASE_URL": "@database_url"
	},
	"build": {
		"env": {
			"DATABASE_URL": "@database_url"
		}
	},
	"scripts": {
		"build": "tsc && prisma generate",
		"postbuild": "prisma migrate deploy"
	}
}
